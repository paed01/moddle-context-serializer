{
  "name": "moddle-context-serializer",
  "version": "3.1.1",
  "description": "Make bpmn-moddle context serializable and mapped to behaviour functions",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint && npm run dist",
    "test:lcov": "c8 -r lcov mocha && npm run lint",
    "cov:html": "c8 -r html -r text mocha",
    "lint": "eslint . --cache",
    "dist": "babel src/index.js -o dist/index.cjs",
    "prepack": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/moddle-context-serializer"
  },
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "license": "MIT",
  "files": [
    "dist/index.cjs",
    "src/index.js"
  ],
  "keywords": [
    "bpmn",
    "bpmn 2",
    "serializable",
    "deserializable",
    "isomorphic"
  ],
  "peerDependencies": {
    "bpmn-moddle": ">=6"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "bpmn-moddle": "^8.0.0",
    "bpmn-moddle-6": "npm:bpmn-moddle@6",
    "bpmn-moddle-7": "npm:bpmn-moddle@7",
    "c8": "^7.12.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "camunda-bpmn-moddle-6": "npm:camunda-bpmn-moddle@6",
    "chai": "^4.3.7",
    "eslint": "^8.31.0",
    "mocha": "^10.2.0"
  }
}
