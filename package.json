{
  "name": "moddle-context-serializer",
  "version": "3.2.0",
  "description": "Make bpmn-moddle context serializable and mapped to behaviour functions",
  "type": "module",
  "main": "main.cjs",
  "module": "index.js",
  "typings": "index.d.ts",
  "exports": {
    "import": "./index.js",
    "require": "./main.cjs"
  },
  "sideEffects": false,
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint && npm run dist",
    "test:lcov": "c8 -r lcov mocha && npm run lint",
    "cov:html": "c8 -r html -r text mocha",
    "lint": "eslint . --cache",
    "dist": "rollup -c",
    "prepack": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/moddle-context-serializer"
  },
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "license": "MIT",
  "files": [
    "index.d.ts",
    "main.cjs",
    "index.js"
  ],
  "keywords": [
    "bpmn",
    "bpmn 2",
    "serializable",
    "deserializable",
    "isomorphic"
  ],
  "peerDependencies": {
    "bpmn-moddle": ">=6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "bpmn-moddle": "^8.0.1",
    "bpmn-moddle-6": "npm:bpmn-moddle@6",
    "bpmn-moddle-7": "npm:bpmn-moddle@7",
    "c8": "^7.13.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "camunda-bpmn-moddle-6": "npm:camunda-bpmn-moddle@6",
    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "rollup": "^3.20.0"
  }
}
