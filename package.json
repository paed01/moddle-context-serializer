{
  "name": "moddle-context-serializer",
  "version": "4.2.1",
  "description": "Make bpmn-moddle context serializable and mapped to behaviour functions",
  "type": "module",
  "main": "main.cjs",
  "module": "index.js",
  "browser": "main.cjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint && npm run dist && texample ./README.md,API.md",
    "test:lcov": "c8 -r lcov mocha && npm run lint",
    "cov:html": "c8 -r html -r text mocha",
    "lint": "eslint . --cache && prettier . -c --cache",
    "dist": "rollup -c",
    "prepack": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/moddle-context-serializer"
  },
  "author": {
    "name": "PÃ¥l Edman",
    "url": "https://github.com/paed01"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts",
    "main.cjs"
  ],
  "keywords": [
    "bpmn",
    "bpmn 2",
    "serializable",
    "deserializable",
    "isomorphic"
  ],
  "peerDependencies": {
    "bpmn-moddle": ">=6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@types/bpmn-moddle": "^5.1.11",
    "bpmn-elements": "^14.1.0",
    "bpmn-moddle": "^9.0.1",
    "bpmn-moddle-6": "npm:bpmn-moddle@6",
    "bpmn-moddle-7": "npm:bpmn-moddle@7",
    "c8": "^10.1.2",
    "camunda-bpmn-moddle": "^7.0.1",
    "camunda-bpmn-moddle-6": "npm:camunda-bpmn-moddle@6",
    "chai": "^5.0.0",
    "eslint": "^9.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "rollup": "^4.19.2",
    "texample": "^0.0.6"
  }
}
